import unittest

from lib.globals3 import STATE_OK, STATE_UNKNOWN, STATE_WARN, STATE_CRIT
import lib.base3
import lib.shell3

        
class TestCheck(unittest.TestCase):

    check = './starface-java-memory-usage3'

    def test_if_check_runs_EXAMPLE01(self):
        stdout, stderr, retc = lib.base3.coe(lib.shell3.shell_exec(self.check + ' --test=examples/EXAMPLE01,,'))
        # self.assertEqual
        # self.assertIn
        # self.assertRegexpMatches
        self.assertIn('Heap used: 4.8% (254.0MiB of 5.2GiB), Non-Heap used: 290.5MiB (unlimited memory usage allowed)', stdout)
        self.assertEqual(stderr, '')
        self.assertEqual(retc, STATE_OK)


if __name__ == '__main__':
    unittest.main()
