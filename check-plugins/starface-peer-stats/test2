import unittest

from lib.globals2 import STATE_OK, STATE_UNKNOWN, STATE_WARN, STATE_CRIT
import lib.base2

        
class TestCheck(unittest.TestCase):

    check = './starface-peer-stats2'

    def test_if_check_runs_EXAMPLE01(self):
        stdout, stderr, retc = lib.base2.coe(lib.base2.shell_exec(self.check + ' --test=examples/EXAMPLE01,,'))
        # self.assertEqual
        # self.assertIn
        # self.assertRegexpMatches
        self.assertIn('169 SIP Peers: 100 online / 68 offline (monitored), 1 online / 0 offline (unmonitored)', stdout)
        self.assertEqual(stderr, '')
        self.assertEqual(retc, STATE_OK)


if __name__ == '__main__':
    unittest.main()
